{
  "name": "koffi-curl",
  "version": "0.1.0",
  "description": "Node.js libcurl bindings using koffi with browser fingerprint capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "lib",
    "scripts"
  ],
  "scripts": {
    "build": "tsc",
    "prepublish": "npm run build",
    "test": "npm run build && node tests/run-tests.js",
    "postinstall": "node scripts/postinstall.js"
  },
  "keywords": [
    "curl",
    "http",
    "libcurl",
    "curl-impersonate",
    "fingerprint",
    "koffi",
    "ffi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "got": "^11.8.6",
    "koffi": "^2.6.14",
    "tar": "^6.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.17.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
